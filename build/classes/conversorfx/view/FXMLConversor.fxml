<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="151.0" prefWidth="315.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="conversorfx.controller.FXMLConversorController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Ingrese la Temperatura:" />
      <Button fx:id="btnConvertir" layoutX="95.0" layoutY="96.0" mnemonicParsing="false" onAction="#convertir_OnAction" prefHeight="26.0" prefWidth="125.0" text="Convertir" />
      <TextField fx:id="txfCelsius" layoutX="39.0" layoutY="50.0" onKeyTyped="#borrarOtro_OnKeyTyped" prefHeight="26.0" prefWidth="100.0" promptText="C°" />
      <TextField fx:id="txfFahrenheit" layoutX="180.0" layoutY="50.0" onKeyTyped="#borrarOtro_OnKeyTyped" prefHeight="26.0" prefWidth="100.0" promptText="F°" />
   </children>
</AnchorPane>
